{
  "rewrites": [
    {
      "source": "/api/",
      "destination": "https://new-back-rho.vercel.app/"
    },
    {
      "source": "/api/:match*",
      "destination": "https://new-back-rho.vercel.app/api/:match*"
    },
    {
      "source": "/",
      "destination": "https://baked-goodies-server.vercel.app/"
    },
    {
      "source": "/:match*",
      "destination": "https://baked-goodies-server.vercel.app/:match*"
    }
  ]
}
